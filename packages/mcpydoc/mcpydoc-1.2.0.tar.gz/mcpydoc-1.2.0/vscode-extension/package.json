    "cline",
    "github copilot",
    "private packages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amit608/MCPyDoc.git"
  },
  "bugs": {
    "url": "https://github.com/amit608/MCPyDoc/issues"
  },
  "homepage": "https://github.com/amit608/MCPyDoc#readme",
