<a class="btn btn-${view.status} ${view.css_class} hint"
   data-original-title="${view.hint}"
   data-placement="${view.hint_placement}"
   data-toggle="dropdown">
	<i tal:condition="view.icon_class"
			class="${view.icon_class} fa-fw ${'mr-1' if view.label else ''}"></i>
	<i tal:omit-tag="" i18n:translate="">${view.label}</i>
</a>
<div class="dropdown-menu dropdown-menu-right p-1 calendar-legend"
	 i18n:domain="pyams_app_msc">
	<ul class="m-0 px-1">
		<li class="calendar-marker session-temporary text-nowrap my-1" i18n:translate="">Temporary session</li>
		<li class="calendar-marker session-not-bookable text-nowrap my-1" i18n:translate="">Not bookable session</li>
		<li class="calendar-marker session-internal-bookable text-nowrap my-1" i18n:translate="">Internal bookable session</li>
		<li class="calendar-marker session-external-bookable text-nowrap my-1" i18n:translate="">External bookable session</li>
		<li class="calendar-marker session-confirmed text-nowrap my-1" i18n:translate="">Session with confirmed seats</li>
		<li class="calendar-marker session-full text-nowrap my-1" i18n:translate="">Full session</li>
		<li class="calendar-marker session-public text-nowrap my-1" i18n:translate="">Public session</li>
	</ul>
</div>