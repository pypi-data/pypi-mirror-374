[MAIN]
jobs=0
suggestion-mode=yes
disable=missing-module-docstring,missing-class-docstring,missing-function-docstring,too-few-public-methods,too-many-positional-arguments,protected-access
extension-pkg-whitelist=pygit2

[BASIC]
good-names=i,j,k,fp,s

[FORMAT]
expected-line-ending-format=LF
max-line-length=160
indent-string='    '
