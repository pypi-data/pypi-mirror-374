global:
  SC_2D_Nrad: 30
  SC_2D_Nlong: 45
  SC_3D_Nxf: 8
  SC_3D_Nyf: 8
  SC_3D_Nzf: 8
  accuracy: 6
  initial_distribution: '../1k-250pC-76fsrms-1mm_TE09fixN12.ini'
  ASTRAsettings:
    High_res: True
    N_red: 1
    EmitS: True
    PhaseS: True
    TrackS: True
    RefS: True
    Track_All: True
    Phase_Scan: False
    Auto_Phase: True
    check_ref_part: False
    Zphase: 1
    Zemit: 1050
    H_max: 0.0007
    H_min: 0.0007
    title: 'trial'
    run_no: 1
    Smooth_x: 2
    Smooth_y: 2
    Smooth_z: 2
    Max_scale: 0.1
    Max_count: 100
    cell_var: 2
    min_grid: 3.424657e-13
    Lmagnetized: False
files:
