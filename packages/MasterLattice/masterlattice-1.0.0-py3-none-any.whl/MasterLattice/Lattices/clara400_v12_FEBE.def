    code: elegant
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-S07-SIM-APER-01
      end_element: CLA-FEA-SIM-START-01
  FEBE:
    code: elegant
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-FEA-SIM-START-01
      end_element: CLA-FED-DUMP-01
