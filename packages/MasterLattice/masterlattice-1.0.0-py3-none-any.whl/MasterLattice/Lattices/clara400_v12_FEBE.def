    code: ASTRA
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
      prefix: ''
    output:
      start_element: CLA-S06-SIM-MARK-01
      end_element: CLA-L04-APER
  L04:
    code: ASTRA
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
