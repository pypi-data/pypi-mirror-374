    type: chicane
    elements: [CLA-S04-MAG-DIP-01, CLA-S04-MAG-DIP-02, CLA-S04-MAG-DIP-03, CLA-S04-MAG-DIP-04]
  bunch_compressor:
    type: chicane
    elements: [CLA-VBC-MAG-DIP-01, CLA-VBC-MAG-DIP-02, CLA-VBC-MAG-DIP-03, CLA-VBC-MAG-DIP-04]
elements:
  filename: [YAML/Injector400.yaml, YAML/S02.yaml, YAML/L02.yaml,
             YAML/S03.yaml, YAML/L03.yaml, YAML/S04.yaml, YAML/L4H.yaml,
             YAML/S05.yaml, YAML/VBC.yaml, YAML/S06.yaml, YAML/L04.yaml,
             YAML/S07_FEBE.yaml, YAML/FEBE_ARC.yaml, YAML/FEBE.yaml]
