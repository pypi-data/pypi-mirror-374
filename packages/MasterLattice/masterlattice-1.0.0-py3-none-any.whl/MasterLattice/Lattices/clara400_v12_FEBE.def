    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-S05-APER
      end_element: CLA-S05-SIM-MARK-01
  VBC:
    code: csrtrack
    charge:
      cathode: False
      space_charge_mode: 3D
    csr:
      csr_mode: 1D
    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-S05-SIM-MARK-01
      end_element: CLA-S06-SIM-MARK-01
