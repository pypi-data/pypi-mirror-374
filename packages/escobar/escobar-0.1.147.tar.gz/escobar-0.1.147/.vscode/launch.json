{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug JupyterLab (Grisha: Python)",
      "type": "debugpy",
      "request": "launch",
      "python": "${workspaceFolder}/.venv/bin/python3",
      "module": "jupyter",
      "args": [
        "lab",
        "--no-browser",
        "--NotebookApp.token=''",
        "--NotebookApp.password=''"
      ],
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug JupyterLab Extension",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8888/lab",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack://escobar/*": "${workspaceFolder}/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack:///./*": "${workspaceFolder}/*",
        "webpack:///src/*": "${workspaceFolder}/src/*",
        "webpack:///./src/*": "${workspaceFolder}/src/*"
      },
      "sourceMaps": true,
      "smartStep": true
    },
    {
      "name": "Debug JupyterLab (Roma: Python)",
      "type": "debugpy",
      "request": "launch",
      "python": "/Users/roman/.pyenv/shims/python3",
      "module": "jupyter",
      "args": [
        "lab",
        "--no-browser",
        "--NotebookApp.token=''",
        "--NotebookApp.password=''"
      ],
      "cwd": "/Users/roman/DEVEL/COVER_SELF_1/HTML_PARSE/",
      "justMyCode": false
    }
  ]
}