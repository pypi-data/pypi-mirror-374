{
  "common-conf": {
    "base-directory-path": "gs://stock-prod-lake/transforming",
    "base-reader-name": "iceberg",
    "expectation-data-asset-name": "stock_loss",
    "expectation-datasource-name": "lake-transforming",
    "expectation-run-name": "validate-transforming",
    "path-generator-name": "daily_single_path",
    "project-name": "stock-loss",
    "task-datetime-format": "%Y-%m-%d %H:%M:%S%z"
  },
  "expectation-ge-validation-conf": [
    {
      "args": {
        "column_set": [
          "_id",
          "sa_document_number",
          "ap_document_number",
          "document_number",
          "created_date",
          "sku_list",
          "created_by",
          "branch_code"
        ],
        "exact_match": false
      },
      "expect_rule": "expect_table_columns_to_match_set"
    },
    {
      "args": {
        "column": "_id"
      },
      "expect_rule": "expect_column_values_to_not_be_null"
    },
    {
      "args": {
        "column": "sa_document_number"
      },
      "expect_rule": "expect_column_values_to_not_be_null"
    },
    {
      "args": {
        "column": "_id",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "sa_document_number",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "ap_document_number",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "document_number",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "created_date",
        "type_": "TimestampType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "sku_list",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "created_by",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    },
    {
      "args": {
        "column": "branch_code",
        "type_": "StringType"
      },
      "expect_rule": "expect_column_values_to_be_of_type"
    }
  ],
  "hadoop-conf": {
    "fs.gs.project.id": "stock-data-prod",
    "google.cloud.auth.service.account.json.keyfile": "/cred/data-platform-prod-xxxx-xxxx.json",
    "google.cloud.auth.type": "SERVICE_ACCOUNT_JSON_KEYFILE"
  },
  "iceberg-options": {
    "filter": "CAST(created_date as DATE) = '{YEAR}-{MONTH}-{DAY}'",
    "table-name": "stock_loss"
  },
  "spark-conf": {
    "spark.sql.catalog.local.warehouse": "gs://stock-prod-lake/transforming"
  }
}
