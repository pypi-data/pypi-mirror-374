[distutils]
index-servers = 
    pypi
    testpypi

[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
# password = <PYPI_API_TOKEN> (set via environment variable or CI secrets)

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
# password = <TEST_PYPI_API_TOKEN> (set via environment variable or CI secrets)