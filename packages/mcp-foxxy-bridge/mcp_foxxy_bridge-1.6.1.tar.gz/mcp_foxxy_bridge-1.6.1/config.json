{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Users/billybryant/github/"],
      "tags": ["development", "local", "files"],
      "log_level": "WARNING"
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "tags": ["development", "remote", "git"],
      "enabled": false,
      "log_level": "WARNING"
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "tags": ["development", "web", "http"],
      "enabled": false,
      "log_level": "WARNING"
    },
    "atlassian": {
      "transport": "sse",
      "url": "https://mcp.atlassian.com/v1/sse",
      "tags": ["remote", "atlassian", "jira", "confluence"],
      "enabled": true,
      "log_level": "ERROR",
      "oauth_config": {
        "enabled": true,
        "issuer": "https://auth.atlassian.com",
        "verify_ssl": true
      }
    }
  },
  "bridge": {
    "host": "127.0.0.1",
    "port": 9000,
    "conflictResolution": "namespace",
    "log_level": "INFO",
    "oauth_port": 9090
  }
}
