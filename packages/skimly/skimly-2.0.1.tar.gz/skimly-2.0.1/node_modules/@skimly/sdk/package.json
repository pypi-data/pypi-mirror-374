{
  "name": "@skimly/sdk",
  "version": "0.1.1",
  "description": "Skimly SDK for Node/TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "license": "MIT",
  "keywords": ["skimly", "ai", "sdk", "gateway", "openai", "anthropic"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "node ./scripts/smoke.mjs",
    "prepublishOnly": "npm run build && npm run test"
  },
  "engines": { "node": ">=18" },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "typescript": "^5.5.0"
  }
}
