<xs:schema
  xmlns:sp="urn:spacePacket"
  xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns="http://www.esa.int/safe/sentinel-1.0/"
  targetNamespace="http://www.esa.int/safe/sentinel-1.0/">
  <xs:import namespace="urn:spacePacket" schemaLocation="SpacePacketFormat.dfdl.xsd"/>
  <xs:annotation>
    <xs:appinfo source="http://www.ogf.org/dfdl/">
      <dfdl:format ref="sp:SpacePacketFormat"/>
    </xs:appinfo>
    <xs:documentation>
        <fixedSizeSchema>196</fixedSizeSchema>
    </xs:documentation>
  </xs:annotation>
  <xs:element name="measurements" type="ispType"/>
	<xs:complexType name="ispType">
		<xs:sequence>
			<xs:element name="primaryHeader" type="sp:mainHeaderType"/>
			<xs:element name="secondaryHeader" type="sp:dataFieldHeaderType"/>
      <xs:element name="sourceData" type="navatt"/>
			<xs:element name="errorControl" type="sp:crcType"/>
		</xs:sequence>
	</xs:complexType>
	<!-- === End of dataFieldHeaderType === -->
  <xs:complexType name="navatt">
    <xs:sequence>
      <xs:element name="cswField" type="sp:hexBytes" dfdl:length="4"/>
      <xs:element name="PM_DAT_GNSS_TIME_VALIDITY_FLAG" type="sp:hexBytes" dfdl:length="1"/>
      <xs:element name="PM_DAT_NAVATT_CORRELATION_PPS_OBT" type="sp:cucTime"/>
      <xs:element name="PM_DAT_NAVATT_CORRELATION_GNSS_TIME" type="sp:cucTime"/>
      <xs:element name="SPACECRAFT_CENTRAL_TIME" type="sp:cucTime"/>
      <xs:element name="AO_DAT_I_POS_I_SC_EST_1" type="sp:float64"/>
      <xs:element name="AO_DAT_I_POS_I_SC_EST_2" type="sp:float64"/>
      <xs:element name="AO_DAT_I_POS_I_SC_EST_3" type="sp:float64"/>
      <xs:element name="AO_DAT_I_VEL_I_SC_EST_1" type="sp:float64"/>
      <xs:element name="AO_DAT_I_VEL_I_SC_EST_2" type="sp:float64"/>
      <xs:element name="AO_DAT_I_VEL_I_SC_EST_3" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_I_SC_EST_1" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_I_SC_EST_2" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_I_SC_EST_3" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_I_SC_EST_4" type="sp:float64"/>
      <xs:element name="AO_DAT_SC_RATE_I_SC_EST_1" type="sp:float64"/>
      <xs:element name="AO_DAT_SC_RATE_I_SC_EST_2" type="sp:float64"/>
      <xs:element name="AO_DAT_SC_RATE_I_SC_EST_3" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_SC_ERR_1" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_SC_ERR_2" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_SC_ERR_3" type="sp:float64"/>
      <xs:element name="AO_DAT_Q_SC_ERR_4" type="sp:float64"/>
      <xs:element name="AO_DAT_GDCMODEFLG" type="sp:uint8"/>
      <xs:element name="SPACECRAFT_MODE" type="sp:uint8"/>
      <xs:element name="AO_DAT_THRUST_FLG" type="sp:uint8"/>
      <xs:element name="AO_AJ_IP_OP_FLG" type="sp:uint8"/>
      <xs:element name="AO_DAT_GNSS_VALIDDATA_ITG" type="sp:uint8"/>
      <xs:element name="AO_DAT_NAVATT_ORBITNUMBER_EST_ITG" type="sp:uint32"/>
      <xs:element name="AO_DAT_NAVATT_OOP_EST_ITG" type="sp:uint32"/>
      <xs:element name="AO_DAT_NEWGNSSDATAFLG_ITG" type="sp:uint8"/>
    </xs:sequence>
  </xs:complexType>
	<!-- === End of packet definition === -->
</xs:schema>
